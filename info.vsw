    "id": "report_template",
    "source": "const getCell = (id) => register.cells.find(cell => cell.id === id);\r\n\r\nconst header = getCell('MTY4MjY5MzQ4OTY5MTEwMDg4NDk4').items\r\n\r\nsetAttr('FOR', {Title: register.cells[0].value, Created: new Date(header[2].value).toJSON(), Author: header[0].value, AprovedBy: header[1].value});\r\nconst Relatorio = {\r\n  Numero: 'Nº ' +  getCell('MTY4MDAzNjA3NDQ5ODEwMDg4Nzg3').metadata.report,\r\n  Data: new Date()\r\n}\r\n\r\nsetAttr('Relatorio', Relatorio);\r\nconst cliente = {\r\n  Nome: getCell('MTY4MTI0MzAzMjgyNjEwMDg4NDk4').value,\r\n  Endereço: getCell('MTY4MTI0MzAzNTM2NzEwMDg4NDk4').value,\r\n  CEP: getCell(`MTY4MTIzOTU5ODg3OTEwMDg4NDk4`).items[0].value,\r\n  CNPJ: getCell(`MTY4MTIzOTU5ODg3OTEwMDg4NDk4`).items[1].value\r\n}\r\n\r\nconst fabricante = {\r\n  Nome: getCell('MTY3OTkxNDU1M83735MDEwMDg4Nzg3').value,\r\n  Endereço: getCell('MTY3OTkxND22Mzc5ODEwMDg4Nzg3').value,\r\n  CEP: getCell(`MTY4NDM0NzcwNjM0OTEwMDg4NDk4`).items[0].value,\r\n  CNPJ: getCell(`MTY4NDM0NzcwNjM0OTEwMDg4NDk4`).items[1].value\r\n}\r\n\r\nconst amostra = \r\n{\r\n  Produto: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[0].value,\r\n  Marca: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[1].value,\r\n  Modelo: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[2].value,\r\n  NumeroSerie: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[3].value,\r\n  Carregamento: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[4].value,\r\n  ProtocoloLabelo: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[5].value,\r\n  OrcamentoLabelo: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[6].value,\r\n  Tensao: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[7].value,\r\n  Corrente: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[8].value,\r\n  PotenciaLavagem: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[9].value,\r\n  PotenciaSecagem: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[10].value,\r\n  Frequencia: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[11].value,\r\n  CapacidadeLavagem: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[12].value,\r\n  CapacidadeSecagem: getCell(`MTY3OTkxNTM5MDM1MzEwMDg4Nzg3`).items[13].value\r\n}\r\n\r\nconst detergente_label =getCell(`MTY4MTMyMDc4NDU3MDEwMDg4NDk4`).items\r\n.filter(e=>(e.value && e.value !== \"\"))\r\n.map(e=>`${e.value.split(\":\")[0]}:<br>`)\r\n.join(\"\\n\")\r\n\r\nconst detergente_info =getCell(`MTY4MTMyMDc4NDU3MDEwMDg4NDk4`).items\r\n.filter(e=>(e.value && e.value !== \"\"))\r\n.map(e=>`${e.value.split(\":\")[1]}<br>`)\r\n.join(\"\\n\")\r\n\r\nconst tirasSujidade = getCell(`MTY4MTMyMjEzNTYyMzEwMDg4NDk4`).items\r\n.filter(e=> (e.value && e.value !== \"\"))\r\n.map(e=>`<li>${e.value}</li>`)\r\n.join(\"\\n\")\r\n\r\n\r\nconst lavagem = getCell(`MTY4MTMyMjUzNDc3NDEwMDg4NDk4`).items\r\n.filter(e=> (e.value && e.value !== \"\"))\r\n.map(e=>`<li>${e.value}</li>`)\r\n.join(\"\\n\")\r\n\r\nconst documentos = getCell(`MTY4MTMyNTM0Nzc0MTEwMDg4NDk4`).items\r\n.filter(e=> (e.value && e.value !== \"\"))\r\n.map(e=>`<li>${e.value}</li>`)\r\n.join(\"\\n\")\r\n\r\n\r\nlet observacoes_amostra = getCell('MTY4MTI0NDU2NzIxMjEwMDg4NDk4').value \r\n\r\nlet observacoes_finais = getCell('MTY4MTI0NTAxNTk2MDEwMDg4NDk4').value \r\n\r\nfor (const [key, value] of Object.entries(amostra)) {\r\n  if (value==undefined) {\r\n    amostra[key] = \"Não declarado\"\r\n  }\r\n  \r\n}\r\n\r\n// const objeto = getCell('MTY3OTkxNTM5MDM1MzEwMDg4Nzg3').items.map(item => `${item.title}: ${item.value}`);\r\n\r\n// setAttr(\"Data\", {\r\n//   Inicial: new Date(register.metadata.Created)\r\n// })\r\n\r\n// setAttr('Relatorio', {\r\n//   Data: new Date(),\r\n//   Numero: getCell('MTY4MDAzNjA3NDQ5ODEwMDg4Nzg3').metadata.report\r\n// })\r\n// setAttr('Cliente', cliente);\r\n// setAttr('Objetos', [objeto]);\r\n\r\nreturn `\r\n<div class=\"hide-on-vscode\">\r\n<h1 class=\"title\">Cliente:</h1>\r\n${cliente.Nome}<br>\r\n${cliente.Endereço}<br>\r\nCEP: ${cliente.CEP}<br>\r\nCNPJ: ${cliente.CNPJ}<br>\r\n\r\n\r\n<h2 class=\"title\">Fabricante:</h2>\r\n${fabricante.Nome}<br>\